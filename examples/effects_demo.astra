module effects_demo

fn add(a: Int, b: Int) -> Int
{
  a + b
}

fn greet(name: Text)
  effects(Console)
{
  Console.println("Hello, " + name + "!")
}

fn log_sum(a: Int, b: Int) -> Int
  effects(Console)
{
  let result = add(a, b)
  Console.println("Computing sum...")
  result
}

fn main()
  effects(Console)
{
  let sum = add(2, 3)
  greet("Astra")
  let logged_result = log_sum(10, 20)
  Console.println("Done!")
}
