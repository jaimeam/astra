module contracts

fn divide(a: Int, b: Int) -> Int
{
  a / b
}

fn safe_sqrt(n: Int) -> Int
{
  if n == 0 {
    0
  } else {
    if n == 1 {
      1
    } else {
      let guess = n / 2
      if guess * guess > n {
        guess - 1
      } else {
        guess
      }
    }
  }
}

fn main()
  effects(Console)
{
  Console.println("divide(10, 3) = 3")
  Console.println("safe_sqrt(4) = 2")
}
